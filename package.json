{
  "name": "md-chat-workshop",
  "description": "An HTML5 chat based on Angular.js and firebase.io",
  "version": "0.1.0",
  "repository": "https://github.com/gionkunz/md-chat-workshop.git",
  "dependencies": {},
  "devDependencies": {
    "jshint-stylish": "~0.2.0",
    "grunt": "~0.4.5",
    "load-grunt-tasks": "~0.4.0",
    "time-grunt": "~0.3.2",
    "grunt-contrib-jasmine": "~0.6.5",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-wiredep": "~1.7.0",
    "grunt-svgmin": "~0.4.0",
    "grunt-sass": "~0.13.1",
    "grunt-contrib-connect": "~0.7.1",
    "grunt-contrib-uglify": "~0.4.0",
    "grunt-ngmin": "0.0.3",
    "grunt-contrib-coffee": "~0.10.1",
    "grunt-contrib-imagemin": "~0.7.1",
    "grunt-autoprefixer": "~0.7.4",
    "grunt-usemin": "~2.1.1",
    "grunt-newer": "~0.7.0",
    "grunt-contrib-cssmin": "~0.9.0",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-htmlmin": "~0.3.0",
    "grunt-contrib-concat": "~0.4.0",
    "grunt-concurrent": "~0.5.0",
    "grunt-contrib-watch": "~0.6.1"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "test": "grunt test"
  },
  "config": {
    "source": "source",
    "dist": "dist",
    "minifyHtml": true,
    "prepareForAngular": true,
    "connectHost": "0.0.0.0",
    "connectPort": "9000",
    "sassIncludePaths": [
      "source/bower_components",
      "source/bower_components/compass-mixins/lib"
    ],
    "bowerExclude": [
      "foundation.css",
      "es5-shim.js",
      "json3.min.js"
    ],
    "test": {
      "testResources": [
        "source/bower_components/jquery/dist/jquery.min.js",
        "source/bower_components/fastclick/lib/fastclick.js",
        "source/bower_components/foundation/js/foundation.min.js",
        "source/bower_components/angular/angular.min.js",
        "source/bower_components/firebase/firebase.js",
        "source/bower_components/angularfire/angularfire.min.js",
        "source/scripts/**/*.js"
      ]
    }
  }
}
